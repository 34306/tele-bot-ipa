# Telegram Bot Configuration
TELEGRAM_API_ID=your_api_id_here
TELEGRAM_API_HASH=your_api_hash_here
TELEGRAM_BOT_TOKEN=your_bot_token_here
ADMIN_IDS=your_telegram_user_id_here

# SSH Configuration for iPhone
SSH_HOST=192.168.x.x
SSH_PORT=22
SSH_USER=root
SSH_PASSWORD=alpine

# Paths on iPhone
IPA_DOWNLOAD_PATH=/var/mobile/Documents/ipa
IPA_DECRYPTED_PATH=/var/mobile/Documents/decrypted
APP_BUNDLE_PATH=/private/var/containers/Bundle/Application

# Temporary paths
TEMP_WORK_PATH=/var/tmp

# Optional: ipatool path
IPATOOL_PATH=ipatool

# Telegram User Session (for uploading files to backup channel)
TELEGRAM_API_ID=your_api_id
TELEGRAM_API_HASH=your_api_hash
USER_SESSION_STRING=your_session_string
BACKUP_CHANNEL_ID=-100XXXXXXXXX

# Device UDID (optional, for ideviceinstaller when multiple devices connected)
DEVICE_UDID=devicde-udid


# REMEMBER TO SET THIS FILE TO .env WHEN YOU'RE CONFIG IT DONE.